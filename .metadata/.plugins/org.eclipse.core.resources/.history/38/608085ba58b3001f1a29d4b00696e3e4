package zoo;

public class App {

	public static void main(String[] args) {
		Gato gato = new Gato();
		Jacare jaca = new Jacare();
		Cachorro cao = new Cachorro();
		
		jaca.setNome("Jacar√©");
		jaca.setRaca("albino");
		jaca.setPeso(50);
		System.out.println(jaca.getNome());
		jaca.brincar();
		System.out.println(jaca.getPeso());
		gato.setNome("Gato");
		gato.setCor("preto");
		gato.brincar();
		gato.emitirSom();
		gato.saltar();
	}

}
