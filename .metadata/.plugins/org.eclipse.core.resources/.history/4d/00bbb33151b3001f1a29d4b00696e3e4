package zoo;

public class App {

	public static void main(String[] args) {
		Gato gato = new Gato();
		Jacare jaca = new Jacare();
		
		jaca.brincar();
		gato.setCor("Branco");
		gato.setRaca("Vira lata");
		gato.setPeso (-2);
		gato.saltar();
		gato.emitirSom();
		gato.brincar();
		jaca.brincar();
	}

}
