package zoo;

public class Gato {
	String cor;
	String raca;
	double peso;
	
	public void emitirSom() {
		if(peso) {
			peso <= 3 System.out.println("Miau!");
		}
		
	}
	
	public void brincar() {
		System.out.println("brincando...");
	}
	
	public void saltar() {
		System.out.println("saltando...");
	}
}
