package zoo;

public class Animal {
	private String cor;
	private String raca;
	private double peso;
	
	public String getCor() {
		return cor;
	}
	public void setCor(String cor) {
		this.cor = cor;
	}
	public String getRaca() {
		return raca;
	}
	public void setRaca(String raca) {
		this.raca = raca;
	}
	public double getPeso() {
		return peso;
	}
		public void setPeso(double peso) {
			System.out.println();
		}

		public void emitirSom() {
			System.out.println();
		}
		
		public void brincar() {
			System.out.println();
		}
		
		public void saltar() {
			System.out.println();
		}
}
